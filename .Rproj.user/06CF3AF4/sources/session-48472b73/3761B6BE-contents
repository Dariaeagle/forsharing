greet <- function(name) {
  paste0("How do you do, ", name, "?")
}

greet("Martin")
library(EagleTestPackage)
helloDaria()
helloEAGLES()
funndvi <- function(nir, red) {
  (nir - red) / (nir + red)
}

# Example usage:
nir_value <- 0.8
red_value <- 0.6

ndvi_result <- funndvi(nir_value, red_value)
print(ndvi_result)
